{
  "name": "css.gui",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "yarn turbo run clean && rm -rf node_modules",
    "nuke": "yarn clean && yarn",
    "pkg:test": "manypkg check",
    "pkg:fix": "manypkg fix"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.18",
  "dependencies": {
    "@manypkg/cli": "^0.19.1",
    "prettier": "^2.6.2",
    "tsup": "^5.12.5",
    "turbo": "^1.2.2"
  }
}
